public class ClasseAlunoMontanha {

    /**
     * @param args the command line arguments
     */
    public static void main(String[] args) {
        aluno  eu;
        eu = new aluno();
        eu.inicializar("Igor", "125406984", 30);
        if (eu.media()){
            System.out.println("Você passou "+eu.nome);
        }   else{
            System.out.println("Você não passou "+eu.nome);
        }
       
        
                
    }
    
}
